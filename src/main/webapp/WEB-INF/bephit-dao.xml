<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="mainDAO" class="bephit.dao.MainDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="userDAO" class="bephit.dao.UserDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="partDAO" class="bephit.dao.ParticipantGroupDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="adminuserDAO" class="bephit.dao.AdminUserDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="adminactivityDAO" class="bephit.dao.AdminActivityDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="streamDAO" class="bephit.dao.StreamDetailsDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="broadDAO" class="bephit.dao.BroadCastDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="mailTemplateDAO" class="bephit.dao.MailTemplateDAO">
		<property name="dataSource" ref="dataSource"></property>
	</bean>
	<bean id="messagelogdao" class="bephit.dao.MessageLogDAO">
		<property name="dataSource" ref="dataSource"></property>
	</bean>
<bean id="messageSchedulingDAO" class="bephit.dao.MessageSchedulingDAO">
		<property name="dataSource" ref="dataSource"></property>
	</bean>
	<bean id="messageStatusDAO" class="bephit.dao.MessageStatusDAO">
	<property name="dataSource" ref="dataSource"></property>
	</bean>
	<bean id="responseDAO" class="bephit.dao.ResponseDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="participantAnswersDAO" class="bephit.dao.ParticipantAnswersDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
</beans>